using namespace std;
#include<iostream>//to find the particular element in an array
void tofind_size(int * arr,int n,int key)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]==key)
        {
            cout << i;
            break;
        }
        
    }
}
int main()
{
    int n,key;
    cin >> n  >>key;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
    }
    tofind_size(arr,n,key);
    return 0;
    
    
}
